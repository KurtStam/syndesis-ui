{
  "name": "redhat-ipaas-client",
  "version": "1.0.0",
  "description": "Red Hat iPaaS Client",
  "homepage": "https://github.com/redhat-ipaas/ipaas-client",
  "authors": [
    "Stan Lewis <stlewis@redhat.com> (https://github.com/gashcrumb)",
    "Rachel Yord√°n <rachel.yordan@redhat.com> (https://github.com/kahboom)"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:docker": "yarn build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "yarn build:dev",
    "ci": "yarn lint && yarn test && yarn e2e",
    "clean:dist": "yarn rimraf -- dist",
    "clean:install": "yarn install --no-progress",
    "clean:start": "yarn start",
    "clean": "yarn cache clean && yarn rimraf -- node_modules doc coverage dist",
    "docs": "yarn typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "yarn e2e -- --elementExplorer",
    "e2e": "yarn protractor",
    "github-deploy:dev": "webpack --config config/webpack.github-deploy.js --progress --profile --github-dev",
    "github-deploy:prod": "webpack --config config/webpack.github-deploy.js --progress --profile --github-prod",
    "github-deploy": "yarn github-deploy:dev",
    "lint": "yarn tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "yarn clean:dist",
    "prebuild:prod": "yarn clean:dist",
    "preclean:install": "yarn clean",
    "preclean:start": "yarn clean",
    "pree2e": "yarn webdriver:update -- --standalone",
    "preversion": "yarn test",
    "rimraf": "rimraf",
    "server": "yarn server",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --profile --watch --content-base src/",
    "server:dev:hmr": "yarn server:dev -- --inline --hot",
    "server:prod": "http-server dist --cors",
    "start": "yarn server:dev",
    "start:hmr": "yarn server:dev:hmr",
    "test": "karma start",
    "version": "yarn build",
    "watch:dev:hmr": "yarn watch:dev -- --hot",
    "watch:dev": "yarn build:dev -- --watch",
    "watch:prod": "yarn build:prod -- --watch",
    "watch:test": "yarn test -- --auto-watch --no-single-run",
    "watch": "yarn watch:dev",
    "webdriver:start": "yarn webdriver-manager start",
    "webdriver:update": "yarn webdriver-manager update",
    "webpack": "webpack",
    "report-coverage": "codecov -f ${CIRCLE_ARTIFACTS:-.}/coverage/coverage-final.json"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/platform-server": "2.0.0",
    "@angular/router": "3.0.0",
    "@angularclass/conventions-loader": "1.0.12",
    "@angularclass/hmr": "1.2.1",
    "@angularclass/hmr-loader": "3.0.2",
    "@angularclass/request-idle-callback": "1.0.7",
    "@angularclass/webpack-toolkit": "1.3.3",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.5",
    "bootstrap": "4.0.0-alpha.4",
    "c3": "0.4.11",
    "core-js": "2.4.1",
